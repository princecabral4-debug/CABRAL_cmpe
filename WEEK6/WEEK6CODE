#List

fruitsA = ["Apple", "Apple", "Banana", "Orange", "Grapes"]

print(fruitsA)
print(fruitsA)
print(fruitsA[1])
print(fruitsA[2])
print(fruitsA[3])
print(fruitsA.index("Grapes"))

isthere = "mansanas" in fruitsA
print(isthere)
isthere = "Grapes" in fruitsA
print(isthere)
fruitsA.append("Watermelon")
print(fruitsA)
fruitsA.insert(3, "Mango")
print(fruitsA)
print(len(fruitsA))
print(fruitsA)
randomMixList = ["Mango", True, 7]
print(randomMixList)
print(randomMixList[1])

print(fruitsA.count("Apple"))
print(fruitsA.count("apple"))
fruitsA.remove("Apple")
print(fruitsA)
fruitsA.count("Apple")
fruitsA.remove("Apple")
print(fruitsA)
fruitsA[1] = "Buko"
print(fruitsA)
print(fruitsA.index("Orange"))
fruitsA[1] = "Buko"
print(fruitsA)

#LIST #MUTABLE #CRUD -CREATE(append/insert) READ(view)
#UPDATE(assigned) and DELETE(remove/clear)

fruitsA = ["Apple", "Banana", "Orange", "Grapes"]
fruitsB = ["Mansanas", "Saging", "Orange", "Ubas"]
fruitsC = ["Rambutan", "Papaya", "Grapes", "Lychee"]

fruitbasket_list = [fruitsA, fruitsB, fruitsC]
print(fruitbasket_list)

fruitbasket_Plus = fruitsA + fruitsB + fruitsC
print(fruitbasket_Plus)
print(fruitbasket_list) #list of list
print(fruitbasket_list)
print(fruitbasket_list[2])
print(fruitbasket_list[2][1])

fruitsA.extend(fruitsB)
fruitsA.extend(fruitsC)
print(fruitsA) #single list
fruitsA.extend(fruitsB+fruitsC)
print(fruitsA)




#LIST #MUTABLE #CRUD -CREATE(append/insert) READ(view)
#UPDATE(assigned) and DELETE(remove/clear)

fruitsA = {"apple", "apple", "orange", "grapes", "mango"}
print(fruitsA)

fruitsA.add("papaya")
print(fruitsA)
fruitsA = {'grapes', 'mango', 'orange', 'apple', 'papaya'}
print(fruitsA)

fruitsA = {'grapes', 'mango', 'orange', 'apple', 'papaya'}
fruitsB = {'orange', 'apple', 'papaya', 'bayabas', 'mangga'}
fruitsA.add("rambutan")
print(fruitsA)

fruitsUnion = fruitsA.union(fruitsB)
print(fruitsUnion)
fruitsIntersection = fruitsA.intersection(fruitsB)
print(fruitsIntersection)
fruitsDifference = fruitsA.difference(fruitsB)
print(fruitsDifference)


fruitsA = {'grapes', 'mango', 'orange', 'apple', 'papaya'}
fruitsB = {'orange', 'apple', 'papaya', 'bayabas', 'mangga'}
fruitsbasketListOfSet = [fruitsA, fruitsB]
print(fruitsbasketListOfSet)

#TUPLE #IMMUTABLE

fruitsA = ('grapes', 'mango', 'orange', 'apple', 'papaya')
print(fruitsA)

print(fruitsA.index("mango"))
print(fruitsA.count("papaya"))
print(fruitsA[4])

fruitsB = ('orange', 'apple', 'papaya', 'bayabas', 'mangga')
print(fruitsB)
fruitbasket = (fruitsA, fruitsB)
print(fruitbasket)

mytuple = (
    (1, 2 ,3, "A"),
    (4, 5 ,6, "B"),
    (7, 8 ,9, "C"),
    (0, "*", "#", "D")
)
print(mytuple[3][2])


myInfo = {"Name" : "Carl Tamayo", "Age" : 123456789009654321, "Gender" : "Male", "Address" : "Yokohama Japan"}
print(myInfo)
print(myInfo["Age"])
print(myInfo["Gender"])
print(myInfo["Address"])
print(myInfo["Name"])
myInfo["Name"] = "James Reid"
print(myInfo)
print(myInfo["Name"])
myInfo.update({"Section" : 4})
print(myInfo)

